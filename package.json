{
    "name": "@wakecaptech/wakecap-ol",
    "version": "0.0.5-alpha",
    "description": "Functional, composed, extended and react wrapper for OpenLayers",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
        "test:watch": "react-scripts-ts test --env=jsdom",
        "build": "rollup -c",
        "build:reset": "rm -rf ./dist ./build && tsc && rollup -c",
        "start": "rollup -c -w",
        "prepare": "yarn run build"
    },
    "keywords": [
        "react",
        "ol",
        "react-openlayers",
        "react-ol",
        "wakecap"
    ],
    "author": "Wakecap Technologies Inc.,",
    "license": "SEE LICENSE IN LICENSE.md",
    "peerDependencies": {
        "ol": "^6.1.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "lodash": "^4.17.15",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^2.0.1",
        "@svgr/rollup": "^2.4.1",
        "@types/babel-core": "^6.25.6",
        "@types/lodash.throttle": "^4.1.6",
        "@types/ol": "^5.3.6",
        "@types/react": "^16.9.16",
        "@types/react-dom": "^16.9.4",
        "@types/rollup-plugin-peer-deps-external": "^2.2.0",
        "@types/rollup-plugin-postcss": "^2.0.0",
        "@types/rollup-plugin-url": "^2.2.0",
        "@types/svgr__rollup": "^4.3.0",
        "@types/uuid": "^3.4.6",
        "babel-core": "^6.26.3",
        "babel-runtime": "^6.26.0",
        "react-scripts-ts": "^2.16.0",
        "rollup": "^0.62.0",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-commonjs": "^9.1.3",
        "rollup-plugin-node-resolve": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^1.6.2",
        "rollup-plugin-sass": "^1.2.2",
        "rollup-plugin-typescript2": "^0.25.3",
        "rollup-plugin-url": "^1.4.0",
        "typescript": "^2.8.3"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "lodash.throttle": "^4.1.1",
        "ol": "^6.1.1",
        "uuid": "^3.3.3"
